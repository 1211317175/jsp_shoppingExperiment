<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>UserSerlvet</servlet-name>
        <servlet-class>com.study.servlet.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserSerlvet</servlet-name>
        <url-pattern>/UserSerlvet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ItemServlet</servlet-name>
        <servlet-class>com.study.servlet.ItemServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItemServlet</servlet-name>
        <url-pattern>/ItemServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CheckShoppingTrolleyServlet</servlet-name>
        <servlet-class>com.study.servlet.CheckShoppingTrolleyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckShoppingTrolleyServlet</servlet-name>
        <url-pattern>/CheckShoppingTrolleyServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CUDShoppingTrolleyServlet</servlet-name>
        <servlet-class>com.study.servlet.CUDShoppingTrolleyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CUDShoppingTrolleyServlet</servlet-name>
        <url-pattern>/CUDShoppingTrolleyServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OrderFormServlet</servlet-name>
        <servlet-class>com.study.servlet.OrderFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderFormServlet</servlet-name>
        <url-pattern>/OrderFormServlet</url-pattern>
    </servlet-mapping>
</web-app>